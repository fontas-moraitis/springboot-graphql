scalar Date @specifiedBy(url:"https://tools.ietf.org/html/rfc3339")
scalar Url @specifiedBy(url:"https://www.w3.org/Addressing/URL/url-spec.txt")

type Query {
    contents: [Content]
}

type Content {
    id: ID
    title: String
    description: String
    status: String
    contentType: String
    dateCreated: Date
    dateUpdated: Date
    url: Url
}